{
    "cargo": {
        "PSModuleName": "posh-cargo"
    },
    "dcos": {
        "PSModuleName": "posh-dotnet"
    },
    "dotnet": {
        "PSModuleName": "posh-docker"
    },
    "docker": {
        "PSModuleName": "posh-docker"
    },
    "git": {
        "PSModuleName": "posh-git"
    },
    "mvn": {
        "PSModuleName": "MavenAutoCompletion",
        "MinimumPowerShellVersion": "6.0"
    },
    "npm": {
        "PSModuleName": "npm-completion"
    },
    "scoop": {
        "PSModuleName": "scoop-completion",
        "CustomInstallCommand": "scoop bucket add scoop-completion https://github.com/liuzijing97/scoop-completion; scoop install scoop-completion",
        "CustomLoadCommand": "if (!(Test-Path -Path \"$env:SCOOP\")) { $env:SCOOP = \"$env:USERPROFILE\\scoop\" }; Import-Module \"$env:SCOOP\/modules\/scoop-completion\""
    },
    "vsts": {
        "PSModuleName": "posh-vsts-cli"
    },
    "yarn": {
        "PSModuleName": "yarn-completion"
    }
}